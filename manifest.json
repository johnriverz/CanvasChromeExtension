{
  "manifest_version": 3,
  "name": "Canvas++ Prototype",
  "version": "1.0.0",
  "action": {
    "default_popup": "Canvas++.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "js": [
        "Canvas++.js"
      ],
      "matches": [
        "https://developer.chrome.com/docs/extensions/*"
	]
    }
  ]
}
